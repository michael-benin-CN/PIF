NAME = Gestaltung_von_Prozessketten

all: $(NAME).tex
	latex --output-format=pdf $(NAME).tex
	latex --output-format=pdf $(NAME).tex
	make clean
clean:
	rm -f *.out *.toc *.aux *.dvi *.bbl *.blg *.ind *.idx *.ilg *.log *.lot *.lof *.snm *.nav
clean-all:
	rm -f *.out *.toc *.aux *.dvi *.bbl *.blg *.ind *.idx *.ilg *.log *.lot *.lof *.snm *.nav
